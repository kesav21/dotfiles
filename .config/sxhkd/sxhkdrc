
alt + shift + Return
	st

alt + p
	dmenu_run

alt + q
	lockmenu

alt + o; p
	lspdf | dmenu | uniq | xargs -I pdf zathura --fork -e "$(tab)" 'pdf'

alt + o; s
	st -n centered -e zsh

alt + o; c
	st -n centered -e bc -iql

XF86AudioMute
	togglemute && pkill -RTMIN+1 dwmblocks

XF86AudioLowerVolume
	chvol -5% && pkill -RTMIN+1 dwmblocks

XF86AudioRaiseVolume
	chvol +5% && pkill -RTMIN+1 dwmblocks

XF86MonBrightnessDown
	chbright -5% && pkill -RTMIN+2 dwmblocks

XF86MonBrightnessUp
	chbright +5% && pkill -RTMIN+2 dwmblocks

Print
	sshot -u

